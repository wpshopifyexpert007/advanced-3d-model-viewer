{
  "name": "advanced-3d-model-viewer",
  "version": "1.0.0",
  "description": "A powerful 3D Model viewer with Gutenberg block",
  "author": "WP Shopify Expert",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js",
    "i18n-pot": "wp i18n make-pot . i18n/advanced-3d-model-viewer.pot --exclude=\"src, node_modules\" && cp ./i18n/advanced-3d-model-viewer.pot ./i18n/advanced-3d-model-viewer.pot-en_US.po",
    "i18n-json": "wp i18n make-json i18n/ --no-purge",
    "i18n-mo": "wp i18n make-mo i18n/",
    "i18n": "npm run i18n-pot && npm run i18n-json && npm run i18n-mo",
    "bundle": "wp-scripts build && npm run i18n && gulp bundle && gulp plugin_zip"
  },
  "files": [
    "lib",
    "assets",
    "build",
    "includes",
    "advanced-3d-model-viewer.php",
    "vendor/composer",
    "vendor/bin",
    "vendor/autoload.php",
    "!package.json",
    "readme.txt"
  ],
  "devDependencies": {
    "@wordpress/scripts": "^26.0.0",
    "eslint-webpack-plugin": "^4.2.0",
    "gulp": "^5.0.0",
    "gulp-zip": "^6.0.0"
  },
  "dependencies": {
    "@wordpress/block-editor": "^11.0.0",
    "@wordpress/blocks": "^12.0.0",
    "@wordpress/components": "^23.0.0",
    "@wordpress/i18n": "^4.0.0",
    "immer": "^10.1.1"
  }
}
